import java.lang.reflect.Array;
import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public final class FinalClassDemo {
    private final String fName;
    private final String lName;
    private final List<Integer> list = Arrays.asList(10,20,30);

    public FinalClassDemo(String fName, String lName) {
        this.fName = fName;
        this.lName = lName;
        //this.list = list;
    }

    public String getfName() {
        return fName;
    }

    public String getlName() {
        return lName;
    }

    public List<Integer> getList() {
        return new ArrayList<Integer>(this.list);
    }

    @Override
    public String toString() {
        return "FinalClassDemo{" +
            "fName='" + fName + '\'' +
            ", lName='" + lName + '\'' +
            ", list=" + list +
            '}';
    }

    public static void main(String[] args) {
        List<Integer> list = new ArrayList<>();
        list.add(10);
        list.add(20);
        FinalClassDemo obj1 = new FinalClassDemo("Sujeet","kumar");
        obj1.getList().add(30);
        System.out.println(obj1);
    }
}
